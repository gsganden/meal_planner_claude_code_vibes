# Frontend Environment Variables
# Copy this file to .env and update with your values

# API Backend URL
# For local development with backend on port 8000
VITE_API_URL=http://localhost:8000

# For production, set to your Modal backend URL:
# VITE_API_URL=https://your-username--recipe-chat-assistant-fastapi-app.modal.run

# WebSocket URL (optional - will derive from API URL if not set)
# VITE_WS_URL=ws://localhost:8000

# Environment
VITE_ENVIRONMENT=development